%table
  %tr
    %th Lastname
    %th Firstname
    %th Gender
    %th Date of Birth
    %th Favorite Color
  - @users.each do |user|
    %tr
      %td #{user.lastname}
      %td #{user.firstname}
      %td #{user.gender}
      %td #{(user.birthdate).strftime("%m/%d/%Y")}
      %td #{user.favorite_color}
